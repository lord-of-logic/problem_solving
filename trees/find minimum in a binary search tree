#include<iostream>
#include<vector>

using namespace std;

class node
{
public:
    int data;
    node *left;
    node *right;
    node(int data)
    {
        this->data=data;
        this->left=nullptr;
        this->right=nullptr;
    }
};

void insert(node **root,int data)
{
    node *temp_root=*root;
    if(!temp_root)
    {
        temp_root=new node(data);
        *root=temp_root;
        return;
    }
    if(temp_root->data>data)
    {
        insert(&temp_root->left,data);
        return;
    }
    insert(&temp_root->right,data);
    return;
}

node *find_min(node *root)
{
    if(!root)
        return nullptr;
    if(root->left)
        return find_min(root->left);
    return root;
}

int main()
{
    node *root=nullptr;
    vector<int> A={50,15,62,5,20,58,91,3,8,37,60,24};
    for(int i=0;i<A.size();i++)
        insert(&root,A[i]);
    cout<<find_min(root)->data;
    return 0;
}
