#include<iostream>
#include<vector>
#include<queue>

using namespace std;

class node
{
public:
    int data;
    node *left;
    node *right;
    node(int data)
    {
        this->data=data;
        this->left=nullptr;
        this->right=nullptr;
    }
};

void insert(node **root,int data)
{
    node *temp_root=*root;
    if(!temp_root)
    {
        temp_root=new node(data);
        *root=temp_root;
        return;
    }
    if(temp_root->data>data)
    {
        insert(&temp_root->left,data);
        return;
    }
    insert(&temp_root->right,data);
    return;
}

void level_order(queue<node *>& que)
{
    if(que.empty())
        return;
    node *root=que.front();
    que.pop();
    cout<<root->data<<" ";
    if(root->left)
        que.push(root->left);
    if(root->right)
        que.push(root->right);
    level_order(que);
}

int main()
{
    node *root=nullptr;
    vector<int> A={50,15,62,5,20,58,91,3,8,37,60,24,100};
    for(int i=0;i<A.size();i++)
        insert(&root,A[i]);
    cout<<"Level order traversal: ";
    queue<node *> que;
    que.push(root);
    level_order(que);
    return 0;
}
